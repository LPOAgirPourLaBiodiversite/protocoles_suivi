{
  "description_field_name":"cd_nom",
  "label": "observation",
  "label_list": "observations",
  "genre":"F",
"display_properties": [
  "id_nomenclature_observation_status",
  "id_nomenclature_obs_technique",
  "cd_nom",
  "id_nomenclature_stade",
  "id_nomenclature_sex",
  "num_cp_tamis",         
  "larve_stade",
  "larve_long_cm",
