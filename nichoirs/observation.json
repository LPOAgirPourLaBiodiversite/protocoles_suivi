{
  "id_field_name": "id_observation",
  "description_field_name": "id_observation",
  "chained": true,
  "label": "Observation",
  "genre": "F",
  "display_properties": [
    "cd_nom",
    "cd_nom_precision",
    "nidification",
    "adulte",
    "oeuf",
    "jeune_vivant",
    "jeune_mort"
  ],
  "uuid_field_name": "uuid_observation",
  "specific": {
    "uuid_observation": {
      "attribut_label": "uuid"
    },
    "id_observation": {
      "type_widget": "text",
      "attribut_label": "Id observation",
      "hidden": true
    },
    "id_base_visit": {
      "type_widget": "text",
      "attribut_label": "Id visite",
      "hidden": true
    },
    "cd_nom": {
      "type_widget": "taxonomy",
      "attribut_label": "Espèce",
      "type_util": "taxonomy",
      "required": true,
      "id_list": "__MODULE.ID_LIST_TAXONOMY"
    },
    "nidification": {
      "type_widget": "radio",
      "attribut_label": "Occupation",
      "values": ["Oui", "Non"],
      "required": true
    },
    "adulte": {
      "type_widget": "number",
      "attribut_label": "Nombre d'adultes",
      "hidden": "({value}) => value.nidification != 'Oui'",
      "required": false,
      "min": 0,
      "max": 50
    },
    "oeuf": {
      "type_widget": "number",
      "attribut_label": "Nombre d'oeufs",
      "hidden": "({value}) => value.nidification != 'Oui'",
      "required": false,
      "value" : 0,
      "min": 0,
      "max": 20
    },
    "jeune_vivant": {
      "type_widget": "number",
      "attribut_label": "Nombre de jeunes vivants",
      "hidden": "({value}) => value.nidification != 'Oui'",
      "required": false,
      "value" : 0,
      "min": 0,
      "max": 20
    },
    "jeune_mort": {
      "type_widget": "number",
      "attribut_label": "Nombre de jeunes morts",
      "hidden": "({value}) => value.nidification != 'Oui'",
      "required": false,
      "min": 0,
      "max": 20
    },
    "medias": {
      "type_widget": "medias",
      "attribut_label": "Médias",
      "schema_dot_table": "gn_monitoring.t_observations"
    }
  }
}
